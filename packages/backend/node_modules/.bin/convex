#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joshykins/stupid-neko/node_modules/.pnpm/convex@1.26.1_@clerk+clerk-react@5.44.0_react-dom@19.1.1_react@19.1.1__react@19.1.1__react@19.1.1/node_modules/convex/bin/node_modules:/home/joshykins/stupid-neko/node_modules/.pnpm/convex@1.26.1_@clerk+clerk-react@5.44.0_react-dom@19.1.1_react@19.1.1__react@19.1.1__react@19.1.1/node_modules/convex/node_modules:/home/joshykins/stupid-neko/node_modules/.pnpm/convex@1.26.1_@clerk+clerk-react@5.44.0_react-dom@19.1.1_react@19.1.1__react@19.1.1__react@19.1.1/node_modules:/home/joshykins/stupid-neko/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joshykins/stupid-neko/node_modules/.pnpm/convex@1.26.1_@clerk+clerk-react@5.44.0_react-dom@19.1.1_react@19.1.1__react@19.1.1__react@19.1.1/node_modules/convex/bin/node_modules:/home/joshykins/stupid-neko/node_modules/.pnpm/convex@1.26.1_@clerk+clerk-react@5.44.0_react-dom@19.1.1_react@19.1.1__react@19.1.1__react@19.1.1/node_modules/convex/node_modules:/home/joshykins/stupid-neko/node_modules/.pnpm/convex@1.26.1_@clerk+clerk-react@5.44.0_react-dom@19.1.1_react@19.1.1__react@19.1.1__react@19.1.1/node_modules:/home/joshykins/stupid-neko/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../convex/bin/main.js" "$@"
else
  exec node  "$basedir/../convex/bin/main.js" "$@"
fi
