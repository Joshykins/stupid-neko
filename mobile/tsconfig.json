{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "allowJs": true,
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "noEmit": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": [
      "expo",
      "react",
      "react-native"
    ],
    "noUncheckedIndexedAccess": true
  },
  "include": [
    "../apps/mobile/**/*.ts",
    "../apps/mobile/**/*.tsx"
  ],
  "exclude": [
    "../apps/mobile/node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
