{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "build/**", ".next/**", "!**/.next/cache/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {
			"outputs": []
		},
		"test": {
			"outputs": []
		},
		"format:check": {
			"outputs": []
		}
	},
	"globalEnv": [
		"NODE_ENV",
		"NEXT_PUBLIC_CONVEX_URL",
		"EXPO_PUBLIC_CONVEX_URL",
		"CONVEX_SITE_URL",
		"CLERK_PUBLISHABLE_KEY",
		"CLERK_SECRET_KEY",
		"CONVEX_DEPLOYMENT",
		"AUTH_DISCORD_ID",
		"AUTH_DISCORD_SECRET",
		"AUTH_GOOGLE_ID",
		"AUTH_GOOGLE_SECRET",
		"CONVEX_DEPLOY_KEY",
		"CONVEX_URL",
		"DANGEROUS_TESTING",
		"ENV",
		"EXPO_PUBLIC_CONVEX_SITE_URL",
		"EXPO_PUBLIC_DANGEROUS_TESTING",
		"EXPO_PUBLIC_ENV",
		"EXPO_PUBLIC_SITE_URL",
		"EXPO_PUBLIC_SPOTIFY_CLIENT_ID",
		"GEMINI_API_KEY",
		"JWKS",
		"JWT_PRIVATE_KEY",
		"SITE_URL",
		"SPOTIFY_CLIENT_ID",
		"SPOTIFY_CLIENT_SECRET",
		"VITE_CONVEX_SITE_URL",
		"VITE_CONVEX_URL",
		"VITE_DANGEROUS_TESTING",
		"VITE_ENV",
		"VITE_SITE_URL",
		"VITE_SPOTIFY_CLIENT_ID",
		"YOUTUBE_API_KEY"
	],
	"ui": "tui"
}
